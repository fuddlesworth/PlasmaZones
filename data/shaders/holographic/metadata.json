{
    "id": "holographic",
    "name": "Holographic",
    "description": "Sci-fi hologram projection with RGB separation, data streams, shimmer, and glitch effects",
    "author": "PlasmaZones",
    "version": "2.1",
    "renderType": "rendernode",
    "fragmentShader": "effect.glsl",
    "vertexShader": "zone.vert.glsl",
    "parameters": [
        {
            "id": "rgbSeparation",
            "name": "RGB Separation",
            "type": "float",
            "slot": 0,
            "default": 8.0,
            "min": 3.0,
            "max": 15.0
        },
        {
            "id": "projectionLines",
            "name": "Projection Lines",
            "type": "float",
            "slot": 1,
            "default": 0.4,
            "min": 0.2,
            "max": 0.8
        },
        {
            "id": "glitchAmount",
            "name": "Glitch Amount",
            "type": "float",
            "slot": 2,
            "default": 0.2,
            "min": 0.0,
            "max": 0.5
        },
        {
            "id": "shimmerSpeed",
            "name": "Shimmer Speed",
            "type": "float",
            "slot": 3,
            "default": 2.0,
            "min": 1.0,
            "max": 4.0
        },
        {
            "id": "baseOpacity",
            "name": "Hologram Opacity",
            "type": "float",
            "slot": 4,
            "default": 0.75,
            "min": 0.5,
            "max": 0.9
        },
        {
            "id": "dataStreamIntensity",
            "name": "Data Stream",
            "type": "float",
            "slot": 5,
            "default": 0.6,
            "min": 0.0,
            "max": 1.0
        },
        {
            "id": "hologramColor",
            "name": "Hologram Color",
            "type": "color",
            "slot": 0,
            "default": "#00ffcc"
        },
        {
            "id": "accentColor",
            "name": "Accent Color",
            "type": "color",
            "slot": 1,
            "default": "#ff00ff"
        }
    ]
}
